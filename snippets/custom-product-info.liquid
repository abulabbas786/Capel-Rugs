{% capture 'rectangle' %}
<svg width="40px" height="38px" viewBox="0 0 64 64" xmlns="http://www.w3.org/2000/svg">
  <rect x="1" y="12" fill="none" stroke="#000000" stroke-width="2" stroke-miterlimit="10" width="62" height="40"/>
